<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lintex9527.android.sendbroadcastapp">

    <!--自定义了一个权限-->
    <permission
        android:name="com.lintex9527.android.permission.WEATHER_STATE"
        android:label="@string/perlab_weatherchange"
        android:description="@string/perdesc_weatherchange"
        android:protectionLevel="normal"
        />


    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <uses-permission android:name="com.lintex9527.android.permission.WEATHER_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>